{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "arrowFunctions": true
        }
    },
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    },
    "rules": {
        "semi": 1,
        "no-eval": true,
        "no-tabs": true,
        "no-var": 1
    }
}
